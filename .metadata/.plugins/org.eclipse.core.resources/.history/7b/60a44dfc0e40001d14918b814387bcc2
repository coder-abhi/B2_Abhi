package com.acts.matrix;

public class MultiplicationMatrix {
	public static int[][] Multiply(int[][] matrix1, int[][] matrix2,int n) {
		int result[][] = new int[n][n];
		
		
		for(int i=0;i<n;i++) {
			for(int j = 0;j<n;j++) {
				for(int k=0;k<n;k++) {
					result[i][j] += matrix1[k][i] + matrix2[i][k];					
				}
			}
		}
		
		return result;
	}
}
